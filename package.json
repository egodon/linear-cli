{
  "name": "@egcli/lr",
  "description": "",
  "version": "0.14.1",
  "author": "Evan Godon @egodon",
  "bin": {
    "linear": "bin/run",
    "lr": "bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "bugs": "https://github.com/egodon/linear-cli/issues",
  "dependencies": {
    "@linear/sdk": "^1.8.4",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3",
    "@types/inquirer": "^7.3.1",
    "ansi-escapes": "^4.3.1",
    "boxen": "^5.0.0",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "clipboardy": "^2.3.0",
    "dayjs": "^1.10.4",
    "graphql": "^15.5.0",
    "inquirer": "^8.0.0",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "marked": "^2.0.1",
    "marked-terminal": "^4.1.1",
    "ora": "^5.4.0",
    "string-width": "^4.2.2",
    "terminal-link": "^2.1.1",
    "tslib": "^2.2.0",
    "wrap-ansi": "^7.0.0",
    "zod": "1.11"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-document-nodes": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@oclif/dev-cli": "^1",
    "@types/marked": "^2.0.0",
    "@types/node": "^14.14.37",
    "eslint": "^7.24.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "globby": "^11.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.0-beta"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/egodon/linear-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "dirname": "lr",
    "commands": "./lib/commands",
    "bin": "lr",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "config": {
        "description": "View and delete config file"
      },
      "issue": {
        "description": "Create, update and view issues"
      },
      "workspace": {
        "description": "Add or switch to a new workspace"
      }
    }
  },
  "repository": "evangodon/linear-cli",
  "types": "lib/index.d.ts"
}
