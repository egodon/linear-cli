{
  "name": "@egcli/lr",
  "description": "",
  "version": "0.17.0",
  "author": "Evan Godon @egodon",
  "bin": {
    "linear": "bin/run",
    "lr": "bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "bugs": "https://github.com/egodon/linear-cli/issues",
  "dependencies": {
    "@linear/sdk": "^1.8.4",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^5.1.10",
    "ansi-escapes": "^5.0.0",
    "boxen": "^6.2.1",
    "chalk": "^5.0.0",
    "cli-ux": "^6.0.8",
    "clipboardy": "^3.0.0",
    "dayjs": "^1.10.4",
    "graphql": "^16.2.0",
    "inquirer": "^8.0.0",
    "inquirer-autocomplete-prompt": "^1.3.0",
    "marked": "^4.0.10",
    "marked-terminal": "^5.0.0",
    "ora": "^6.0.1",
    "string-width": "^5.1.0",
    "terminal-link": "^3.0.0",
    "tslib": "^2.2.0",
    "wrap-ansi": "^8.0.1",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-document-nodes": "^2.2.2",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@oclif/dev-cli": "^1",
    "@types/marked": "^4.0.1",
    "@types/node": "^17.0.8",
    "@types/inquirer": "^8.1.3",
    "@types/marked-terminal": "^3.1.3",
    "eslint": "^8.6.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "globby": "^12.1.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.0-beta"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/egodon/linear-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "dirname": "lr",
    "commands": "./lib/commands",
    "bin": "lr",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "config": {
        "description": "View and delete config file"
      },
      "issue": {
        "description": "Create, update and view issues"
      },
      "workspace": {
        "description": "Add or switch to a new workspace"
      }
    }
  },
  "repository": "evangodon/linear-cli",
  "types": "lib/index.d.ts"
}
