{
  "name": "@egodon/lnr",
  "description": "An unofficial CLI for Linear",
  "version": "0.1.0",
  "author": "Evan Godon @egodon",
  "bin": {
    "lnr": "bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "bugs": "https://github.com/egodon/linear-cli/issues",
  "dependencies": {
    "@linear/sdk": "^1.8.4",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "^3",
    "@types/inquirer": "^7.3.1",
    "ansi-escapes": "^4.3.1",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "graphql": "^15.5.0",
    "inquirer": "^8.0.0",
    "marked": "^2.0.1",
    "marked-terminal": "^4.1.1",
    "terminal-link": "^2.1.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-document-nodes": "^1.17.10",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@oclif/dev-cli": "^1",
    "@types/marked": "^2.0.0",
    "@types/node": "^10",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^8.1.0",
    "globby": "^10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/egodon/linear-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "lnr",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "init": "./lib/hooks/init/set-global-config",
      "prerun": "./lib/hooks/prerun/validate-config"
    }
  },
  "repository": "egodon/linear-cli",
  "types": "lib/index.d.ts"
}
